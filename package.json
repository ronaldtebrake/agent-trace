{
  "name": "agent-trace-cli",
  "version": "0.1.0",
  "description": "CLI tool for capturing and analyzing Agent Trace data",
  "type": "module",
  "main": "dist/cli/index.js",
  "bin": {
    "agent-trace": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc && npm run copy-dashboard",
    "copy-dashboard": "mkdir -p dist/dashboard/public && cp src/dashboard/public/index.html dist/dashboard/public/ 2>/dev/null || true",
    "dev": "tsx src/cli/index.ts",
    "prepare": "npm run build && node dist/setup/install-hooks.js",
    "test": "echo 'Tests coming soon'",
    "populate-test": "npm run build && node dist/scripts/populate-test-data.js",
    "dashboard:dev": "npm run build && node dist/cli/index.js dashboard --port 3000"
  },
  "keywords": [
    "agent-trace",
    "ai",
    "code-attribution",
    "cursor",
    "claude-code"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.1.0",
    "zod": "^3.24.0",
    "simple-git": "^3.27.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "src/dashboard/public",
    "README.md"
  ]
}
